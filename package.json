    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.1",
    "grunt-banner": "^0.4.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-uglify": "^0.6.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-shell": "^1.2.1",
    "grunt-string-replace": "^1.2.0",
    "jquery": "^2.1.4",
    "load-grunt-tasks": "^3.3.0",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "node server.js",
