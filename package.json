{
  "name": "splitplayer",
  "author": "Holger Schauf",
  "website": "http://player.splitplay.tv",
  "private": false,
  "version": "1.0.3",
  "engines": {
    "node": "4.1.0"
  },
  "description": "SplitPlayer - highly flexible extendable video player",
